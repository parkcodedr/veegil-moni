            <div class="col-md-12">
              <h2 id="title" class="text-center">Transactions </h2>
              <table class="table">
  <thead>
    <tr>
      <th scope="col">SN</th>
      <th scope="col">Transaction Type</th>
      <th scope="col">Sender</th>
      <th scope="col">Amount</th>
      <th scope="col">Date</th>
    </tr>
  </thead>
  <tbody>

        <% if(transactions.length==0){ %>
                            <tr>
      <td colspan="3">No transaction found</td>
    </tr>
                           
<% }else{%>
<tr>
      <th scope="row"><%=transctions._id %></th>
      <td><%=transctions.transactionType %></td>
      <td><%=transctions.sender %></td>
      <td><%=transctions.amount %></td>
      <td><%=transctions.date %></td>
    </tr>
   <% } %>
   
    
  </tbody>
</table>
              
                   
              
